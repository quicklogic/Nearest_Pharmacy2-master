<?xml version="1.0" encoding="utf-8" ?>
<local:FreshBaseContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Nearest_Pharmacy.Pages.OrderPage"
              xmlns:local="clr-namespace:FreshMvvm;assembly = Nearest_Pharmacy">
    <local:FreshBaseContentPage.Content>
        <StackLayout>
            <StackLayout>
                <Label Text="Способ доставки" FontSize="Large" />
                <Picker ItemsSource="{Binding deliveryMethods}" SelectedItem="{Binding Select}" ItemDisplayBinding="{Binding Name}" Title="Способ доставки" >
                </Picker>
            </StackLayout>
        </StackLayout>
    </local:FreshBaseContentPage.Content>
</local:FreshBaseContentPage>